                android:layout_gravity="center_vertical"
                android:text="@string/price"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

            <TextView
                android:id="@+id/tvItPrice"
                style="@style/WrapWrap"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>
        </RelativeLayout>

        <View
            android:id="@+id/mView"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/padding_margin_4"
            android:background="@color/setting_bac"
            android:visibility="gone"/>

        <RelativeLayout
            android:id="@+id/rlLandPrice"
            style="@style/MatchWrap"
            android:layout_marginLeft="@dimen/padding_margin_14"
            android:layout_marginRight="@dimen/padding_margin_14"
            android:paddingBottom="@dimen/padding_margin_14"
            android:paddingTop="@dimen/padding_margin_14"
            android:visibility="gone">

            <TextView
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:text="@string/land_price"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

            <TextView
                android:id="@+id/tvLandPrice"
                style="@style/WrapWrap"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:text="0.00"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/padding_margin_4"
            android:background="@color/setting_bac"/>

        <RelativeLayout
            android:id="@+id/rlBuildingPrice"
            style="@style/MatchWrap"
            android:layout_marginLeft="@dimen/padding_margin_14"
