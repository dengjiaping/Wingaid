            android:paddingTop="@dimen/padding_margin_14">

            <TextView
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:text="@string/bathroomnum"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

