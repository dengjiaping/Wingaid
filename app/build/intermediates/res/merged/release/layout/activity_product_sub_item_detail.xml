                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:text="@string/housetype"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivItBedroom"
                    style="@style/WrapWrap"
                    android:layout_gravity="center_vertical"
                    android:background="@color/transparent"
                    android:src="@mipmap/bedroom"/>

                <TextView
                    android:id="@+id/tvItBedRoom"
                    style="@style/WrapWrap"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_13"/>

                <ImageView
                    android:id="@+id/ivItBathroom"
                    style="@style/WrapWrap"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/padding_margin_6"
                    android:src="@mipmap/shower"/>

                <TextView
                    android:id="@+id/tvItBath"
                    style="@style/WrapWrap"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_13"/>

