                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/padding_margin_4"
            android:background="@color/setting_bac"/>

        <RelativeLayout
            style="@style/MatchWrap"
            android:layout_marginLeft="@dimen/padding_margin_14"
            android:layout_marginRight="@dimen/padding_margin_14"
            android:paddingBottom="@dimen/padding_margin_14"
            android:paddingTop="@dimen/padding_margin_14">

            <TextView
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:text="@string/type"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

            <TextView
                android:id="@+id/tvItType"
                style="@style/WrapWrap"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/padding_margin_4"
            android:background="@color/setting_bac"/>

        <RelativeLayout
            style="@style/MatchWrap"
            android:layout_marginLeft="@dimen/padding_margin_14"
            android:layout_marginRight="@dimen/padding_margin_14"
            android:paddingBottom="@dimen/padding_margin_14"
            android:paddingTop="@dimen/padding_margin_14">

            <TextView
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:text="Lot Number"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>

            <TextView
                android:id="@+id/tvItBatchNum"
                style="@style/WrapWrap"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_13"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="@dimen/padding_margin_4"
            android:background="@color/setting_bac"/>

